{
  "name": "tr3x-sdk",
  "version": "0.0.0",
  "description": "tr3x core utils",
  "main": "./dist/index.js",
  "files": [
    "dist/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "fmt": "prettier --no-semi --arrow-parens avoid --trailing-comma none --print-width 80 --write './*.ts' './*/*.ts'",
    "test": "ncc build --target es2020 --out=./test/tmp ./test/test.ts && node ./test/tmp/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuggetdigital/tr3x-sdk.git"
  },
  "keywords": [
    "tr3x",
    "music",
    "license",
    "mart"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuggetdigital/tr3x-sdk/issues"
  },
  "homepage": "https://github.com/nuggetdigital/tr3x-sdk#readme",
  "devDependencies": {
    "@types/node": "15.12.5",
    "@vercel/ncc": "0.28.6",
    "node-fetch": "2.6.1",
    "prettier": "2.3.2",
    "tape": "5.2.1",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "blake3-hash-wasm": "github:nuggetdigital/blake3-hash-wasm#v0.3.3"
  }
}
